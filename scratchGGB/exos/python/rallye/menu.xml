    <category name="Tortue" id="motion"  colour="#4C97FF" secondaryColour="#3373CC">
			<block type="avancer_reculer">
				<value name="NAME">
					<shadow type="math_number">
						<field name="NUM">2</field>
					</shadow>
				</value>
			</block>
			<block type="var_gauche_droite">
				<value name="NAME">
					<shadow type="math_number">
						<field name="NUM">90</field>
					</shadow>
				</value>
			</block>
			<block type="tortue_teleport">
				<value name="ABS">
					<shadow type="math_number">
						<field name="NUM">0</field>
					</shadow>
				</value>
				<value name="ORD">
					<shadow type="math_number">
						<field name="NUM">0</field>
					</shadow>
				</value>
			</block>
			<block type="tortue_viser">
				<value name="ABS">
					<shadow type="math_number">
						<field name="NUM">0</field>
					</shadow>
				</value>
				<value name="ORD">
					<shadow type="math_number">
						<field name="NUM">0</field>
					</shadow>
				</value>
			</block>
            <block type="tortue_pos">
			</block>
			<block type="couleur_remplissage">
				<value name="COLOR">
					<shadow type="colour_picker">
						<field name="COLOUR">#00ff00</field>
					</shadow>
				</value>
			</block>
			<block type="baisser_lever_stylo"></block>
			<block type="debut_fin_remplissage"></block>
    </category>
    <category name="GeoGebra" id="ggb"  colour="#798933" secondaryColour="#798933">
            <block type="ggb_point">
				<value name="NOM">
					<shadow type="text">
						<field name="TEXT">A</field>
					</shadow>
				</value>
				<value name="X">
					<shadow type="math_number">
						<field name="NUM">1</field>
					</shadow>
				</value>
				<value name="Y">
					<shadow type="math_number">
						<field name="NUM">2</field>
					</shadow>
				</value>
			</block>
            <block type="ggb_commande_2pts">
				<value name="NOM">
					<shadow type="text">
						<field name="TEXT"></field>
					</shadow>
				</value>
				<value name="OBJ1">
					<shadow type="text">
						<field name="TEXT"></field>
					</shadow>
				</value>
				<value name="OBJ2">
					<shadow type="text">
						<field name="TEXT"></field>
					</shadow>
				</value>
			</block>
            <block type="ggb_3_arg">
				<value name="NOM">
					<shadow type="text">
						<field name="TEXT"></field>
					</shadow>
				</value>
				<value name="OBJ1">
					<shadow type="text">
						<field name="TEXT"></field>
					</shadow>
				</value>
				<value name="OBJ2">
					<shadow type="text">
						<field name="TEXT"></field>
					</shadow>
				</value>
				<value name="OBJ3">
					<shadow type="text">
						<field name="TEXT"></field>
					</shadow>
				</value>
			</block>
            <block type="ggb_intersections">
				<field name="OP">Intersection</field>
				<value name="NOM">
					<shadow type="text">
						<field name="TEXT"></field>
					</shadow>
				</value>
				<value name="OBJ1">
					<shadow type="text">
						<field name="TEXT"></field>
					</shadow>
				</value>
				<value name="OBJ2">
					<shadow type="text">
						<field name="TEXT"></field>
					</shadow>
				</value>
			</block>
			<block type="ggb_valeur_de">
				<value name="NAME">
					<shadow type="text">
						<field name="TEXT">distance(P,Q)</field>
					</shadow>
				</value>
			</block>
            <block type="ggb_1_arg_fonct">
				<value name="OBJ1">
					<shadow type="text">
						<field name="TEXT">P</field>
					</shadow>
				</value>
			</block>
            <block type="ggb_1_arg">
				<value name="NOM">
					<shadow type="text">
						<field name="TEXT"></field>
					</shadow>
				</value>
				<value name="OBJ1">
					<shadow type="text">
						<field name="TEXT"></field>
					</shadow>
				</value>
			</block>
			<block type="ggb_couleur_remplissage">
				<value name="OBJ1">
					<shadow type="text">
						<field name="TEXT"></field>
					</shadow>
				</value>
				<value name="COLOR">
					<shadow type="colour_picker">
						<field name="COLOUR">#00ff00</field>
					</shadow>
				</value>
			</block>
            <block type="ggb_commande_2pts_fonct">
				<value name="NOM">
					<shadow type="text">
						<field name="TEXT"></field>
					</shadow>
				</value>
				<value name="OBJ1">
					<shadow type="text">
						<field name="TEXT"></field>
					</shadow>
				</value>
				<value name="OBJ2">
					<shadow type="text">
						<field name="TEXT"></field>
					</shadow>
				</value>
			</block>
            		<block type="ggb_setCell">
				<value name="col">
					<shadow type="text">
						<field name="TEXT">A</field>
					</shadow>
				</value>
				<value name="lig">
					<shadow type="math_number">
						<field name="NUM">1</field>
					</shadow>
				</value>
				<value name="valeur">
					<shadow type="text">
						<field name="TEXT"></field>
					</shadow>
				</value>
			</block>
            		<block type="ggb_getCell">
				<value name="col">
					<shadow type="text">
						<field name="TEXT">A</field>
					</shadow>
				</value>
				<value name="lig">
					<shadow type="math_number">
						<field name="NUM">1</field>
					</shadow>
				</value>
			</block>
    </category>
    <category name="%{BKY_CATEGORY_CONTROL}" id="control" colour="#FFAB19" secondaryColour="#CF8B17">
        <block type="control_repeat" id="control_repeat">
            <value name="TIMES">
                <shadow type="math_whole_number">
                    <field name="NUM">10</field>
                </shadow>
            </value>
        </block>
        <block type="control_if" id="control_if"></block>
        <block type="control_if_else" id="control_if_else"></block>
        <block type="control_repeat_until" id="control_repeat_until"></block>
		<block type="controls_for">
			<field name="VARIABLE">variable</field>
			<value name="FROM">
                <shadow type="math_number">
                    <field name="NUM">1</field>
                </shadow>
			</value>
			<value name="TO">
                <shadow type="math_number">
                    <field name="NUM">10</field>
                </shadow>
			</value>
			<value name="BY">
                <shadow type="math_number">
                    <field name="NUM">1</field>
                </shadow>
			</value>
		</block>
    </category>
